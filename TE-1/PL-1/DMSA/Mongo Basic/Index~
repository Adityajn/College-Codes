					"invalidates" : 0,
					"transformBy" : {
						"state" : 1
					},
					"inputStage" : {
						"stage" : "COLLSCAN",
						"filter" : {
							"pop" : {
								"$gt" : 1600
							}
						},
						"nReturned" : 18440,
						"executionTimeMillisEstimate" : 0,
						"works" : 29355,
